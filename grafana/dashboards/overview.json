        "h": 7,
        "w": 8,
        "x": 8,
        "y": 34
      },
      "id": 11,
      "targets": [
        {
          "expr": "sum(container_memory_usage_bytes{name!=\"\"}) by (name)",
          "legendFormat": "{{name}}",
          "refId": "A"
        }
      ],
      "title": "Memory Usage per Container",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "Bps"
